---
title: colinta
header: projects, blogs, and whatnot
---
{% extends 'layouts/header.j2' %}

{% block content %}
{{ super() }}
<h2>Blog</h2>

<ul class="posts">
{% for blog in site.blogs %}
  <li><span>{{ blog.date | date }}</span> &rsaquo; <a href="{{ blog.url }}">{{ blog.title }}</a></li>
{% endfor %}
</ul>

<h2>Projects</h2>

<ul class="posts">
{% for project in site.projects %}
  <li><span>{{ project.date | date }}</span> &rsaquo; <a href="{{ project.url }}">{{ project.title }}</a></li>
{% endfor %}
</ul>
{% endblock %}